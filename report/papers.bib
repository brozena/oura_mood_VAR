	note = {Publisher: SAGE Publications Ltd},
	keywords = {Digital phenotyping, precision medicine, data doubles, data inheritance, data phantoms, personalized health care},
	pages = {20539517211036799},
	file = {SAGE PDF Full Text:/Users/jab/Zotero/storage/MWPCRXKQ/Green and Svendsen - 2021 - Digital phenotyping and data inheritance.pdf:application/pdf},
}

@article{birk2021WhatItWorth,
