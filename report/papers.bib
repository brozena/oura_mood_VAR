	publisher = {Springer International Publishing},
	author = {Kaczmarek-Majer, Katarzyna and Hryniewicz, Olgierd and Opara, Karol R. and Radziszewska, Weronika and Olwert, Anna and Owsiński, Jan W. and Zadrożny, Sławomir},
	editor = {Destercke, Sébastien and Denoeux, Thierry and Gil, María Ángeles and Grzegorzewski, Przemyslaw and Hryniewicz, Olgierd},
	year = {2019},
	doi = {10.1007/978-3-319-97547-4_16},
	note = {Series Title: Advances in Intelligent Systems and Computing},
	pages = {115--123},
	file = {Kaczmarek-Majer et al. - 2019 - Control Charts Designed Using Model Averaging Appr.pdf:/Users/jab/Zotero/storage/VMVDJ9KI/Kaczmarek-Majer et al. - 2019 - Control Charts Designed Using Model Averaging Appr.pdf:application/pdf},
