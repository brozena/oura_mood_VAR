@article{schreiter2020BankAccountOwnership,
	title = {Bank {Account} {Ownership} and {Access} {Among} {In}-{Patients} in {Psychiatric} {Care} in {Berlin}, {Germany}—{A} {Cross}-{Sectional} {Patient} {Survey}},
