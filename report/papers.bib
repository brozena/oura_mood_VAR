	journal = {Public Health Reports},
	author = {Lee, Brian and Dupervil, Brandi and Deputy, Nicholas P. and Duck, Wil and Soroka, Stephen and Bottichio, Lyndsay and Silk, Benjamin and Price, Jason and Sweeney, Patricia and Fuld, Jennifer and Weber, J. Todd and Pollock, Dan},
	month = sep,
