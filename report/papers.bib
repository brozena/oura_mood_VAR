	note = {Publisher: Public Library of Science},
	keywords = {Decision making, Adolescents, Alcohol consumption, Alcoholism, Behavioral and social aspects of health, Impulsivity, Testosterone, Working memory},
	pages = {e0188079},
	file = {Full Text PDF:/Users/jab/Zotero/storage/42ZJH634/Bromberg et al. - 2017 - Episodic future thinking reduces temporal discount.pdf:application/pdf;Snapshot:/Users/jab/Zotero/storage/55N6LQZH/article.html:text/html},
}

@article{odonnell2017DoesGoalRelevant,
	title = {Does {Goal} {Relevant} {Episodic} {Future} {Thinking} {Amplify} the {Effect} on {Delay} {Discounting}?},
