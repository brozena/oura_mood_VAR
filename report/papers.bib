tex.pmcid: null},
	file = {Jenkins et al_2008_Debt, income and mental disorder in the general population.pdf:/Users/jab/Zotero/storage/EMGG445G/Jenkins et al_2008_Debt, income and mental disorder in the general population.pdf:application/pdf},
