	author = {Neacsiu, Andrada D. and Eberle, Jeremy W. and Kramer, Rachel and Wiesmann, Taylor and Linehan, Marsha M.},
	month = aug,
	year = {2014},
	keywords = {Anxiety, Depression, Dialectical behavior therapy, Emotion dysregulation, Transdiagnostic},
	pages = {40--51},
