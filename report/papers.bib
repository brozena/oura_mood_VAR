	keywords = {Bipolar disorder, Behavioral addiction, Compulsive buying},
	pages = {797--802},
	file = {ScienceDirect Full Text PDF:/Users/jab/Zotero/storage/FR3R8EUS/Kesebir et al. - 2012 - Compulsive buying in bipolar disorder Is it a com.pdf:application/pdf;ScienceDirect Snapshot:/Users/jab/Zotero/storage/X2L4NXCR/S0165032711005635.html:text/html},
