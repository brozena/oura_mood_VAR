	file = {Reinares et al. - 2014 - Psychosocial interventions in bipolar disorder Wh.pdf:/Users/jab/Zotero/storage/VM8MTNTV/Reinares et al. - 2014 - Psychosocial interventions in bipolar disorder Wh.pdf:application/pdf},
