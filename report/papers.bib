	month = nov,
	year = {2017},
	pmid = {27890468},
	pmcid = {PMC7027938},
	keywords = {Bipolar Disorder, bipolar disorder, Humans, Attention Deficit Disorder with Hyperactivity, Attention-deficit/hyperactivity disorder, Comorbidity, cross-disorder meta-analysis, genetic correlation, genetic overlap, Genome-Wide Association Study, GWAS},
