	language = {en},
	number = {6251},
	urldate = {2022-01-24},
	journal = {Science},
	author = {{Open Science Collaboration}},
	month = aug,
	year = {2015},
	pages = {aac4716},
	file = {Open Science Collaboration - 2015 - Estimating the reproducibility of psychological sc.pdf:/Users/jab/Zotero/storage/ZQ4SIE7K/Open Science Collaboration - 2015 - Estimating the reproducibility of psychological sc.pdf:application/pdf},
}

