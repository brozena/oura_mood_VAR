	file = {Bos et al. - 2022 - A Web-Based Application for Personalized Ecologica.pdf:/Users/jab/Zotero/storage/LI8ELALM/Bos et al. - 2022 - A Web-Based Application for Personalized Ecologica.pdf:application/pdf},
}

