	author = {Murray, Greg and Thomas, Neil and Michalak, Erin E. and Jones, Steven H. and Lapsley, Sara and Bowe, Steven J. and Foley, Fiona and Fletcher, Kathryn and Perich, Tania and Johnson, Sheri L. and Cotton, Sue and Berk, Lesley and Mihalopoulos, Cathrine and Kyrios, Michael and Berk, Michael},
	year = {2021},
	note = {Place: US
Publisher: American Psychological Association},
	keywords = {Bipolar Disorder, Treatment, Intervention, Mindfulness, Mindfulness-Based Interventions, Psychological Needs, Quality of Life},
	pages = {830--844},
	file = {Accepted Version:/Users/jab/Zotero/storage/DVC2BHN2/Murray et al. - 2021 - Mindfulness-based online intervention to improve q.pdf:application/pdf;Snapshot:/Users/jab/Zotero/storage/88SNCITY/2022-06096-004.html:text/html},
