	pages = {1175--1191},
	file = {Bonawitz et al. - 2017 - Practical Secure Aggregation for Privacy-Preservin.pdf:/Users/jab/Zotero/storage/C4VZDG3Z/Bonawitz et al. - 2017 - Practical Secure Aggregation for Privacy-Preservin.pdf:application/pdf},
