	pages = {79--88},
	file = {Chancellor et al. - 2019 - A Taxonomy of Ethical Tensions in Inferring Mental.pdf:/Users/jab/Zotero/storage/C5BEXCHF/Chancellor et al. - 2019 - A Taxonomy of Ethical Tensions in Inferring Mental.pdf:application/pdf},
