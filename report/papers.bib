	journal = {JMIR Research Protocols},
	author = {Goulding, Evan H. and Dopke, Cynthia A. and Rossom, Rebecca C. and Michaels, Tania and Martin, Clair R. and Ryan, Chloe and Jonathan, Geneva and McBride, Alyssa and Babington, Pamela and Bernstein, Mary and Bank, Andrew and Garborg, C. Spencer and Dinh, Jennifer M. and Begale, Mark and Kwasny, Mary J. and Mohr, David C.},
