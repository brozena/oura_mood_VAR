	keywords = {ADHD, cripping, critical theory, disability studies, literature review, methodology, neurodivergence, neurodiversity},
	pages = {1--21},
	file = {Full Text PDF:/Users/jab/Zotero/storage/3EGWW5IJ/Spiel et al. - 2022 - ADHD and Technology Research – Investigated by Neu.pdf:application/pdf},
}

